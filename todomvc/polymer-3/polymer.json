{
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "builds": [{
    "name": "es6-unbundled",
    "browserCapabilities": ["es2015", "push"],
    "js": {
      "minify": true,
      "compile": false
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": false
    },
    "bundle": false,
    "addServiceWorker": false,
    "addPushManifest": false
  }]
}