{
    "name": "angular-elements",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "build": "npm run minify-html && npm run build-js && npm run minify-js",
        "build-js": "webpack",
        "minify-js": "./node_modules/.bin/uglifyjs dist/main.bundle.js -o dist/main.bundle.js",
        "minify-html": "./node_modules/.bin/html-minifier index.html -o dist/index.html --collapse-whitespace --remove-comments",
        "serve": "npm run build && http-server ."
    },
    "private": true,
    "dependencies": {
        "@angular/common": "7.0.3",
        "@angular/compiler": "7.0.3",
        "@angular/core": "7.0.3",
        "@angular/elements": "7.0.3",
        "@angular/forms": "7.0.3",
        "@angular/platform-browser": "7.0.3",
        "@angular/platform-browser-dynamic": "7.0.3",
        "@webcomponents/custom-elements": "^1.2.1",
        "core-js": "2.5.7",
        "mutation-observer": "1.0.3",
        "rxjs": "6.3.3",
        "rxjs-compat": "6.3.3",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular/compiler-cli": "7.0.3",
        "@ngtools/webpack": "7.0.4",
        "compression-webpack-plugin": "2.0.0",
        "html-minifier": "3.5.21",
        "http-server": "0.11.1",
        "typescript": "3.1.6",
        "uglify-es": "^3.3.9",
        "uglifyjs-webpack-plugin": "2.0.1",
        "webpack": "4.25.1",
        "webpack-cli": "3.1.2"
    }
}