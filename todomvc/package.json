{
    "name": "web-components-benchmark",
    "version": "1.0.0",
    "description": "Web Components implementations benchmark",
    "scripts": {
        "serve": "node server.js",
        "benchmark": "node benchmarks/benchmark.js",
        "benchmark:page-load": "node benchmarks/benchmark_page-load.js",
        "benchmark:create-todos": "node benchmarks/benchmark_create-todos.js",
        "benchmark:delete-todos": "node benchmarks/benchmark_delete-todos.js",
        "benchmark:edit-todos": "node benchmarks/benchmark_edit-todos.js",
        "benchmark:tti": "node benchmarks/benchmark_load-metrics.js",
        "install:all": "./install-all.sh",
        "build:angular": "cd angular-elements && npm run build",
        "build:atomico": "cd atomico && npm run build",
        "build:dojo2": "cd dojo2 && npm run build",
        "build:litelement": "cd lit-element && npm run build",
        "build:native": "cd native-shadow-dom && npm run build",
        "build:hyperhtml": "cd native-shadow-dom_hyperHTML && npm run build",
        "build:polymer3": "cd polymer3 && npm run build",
        "build:riot": "cd riot && npm run build",
        "build:skatejs:lithtml": "cd skatejs-lit-html && npm run build",
        "build:skatejs:preact": "cd skatejs-preact && npm run build",
        "build:slim": "cd slim.js && npm run build",
        "build:stencil": "cd stencil && npm run build",
        "build:svelte": "cd svelte && npm run build",
        "build:vue": "cd vue.js && npm run build",
        "build:all": "run-s build:angular build:atomico build:dojo2 build:litelement build:native build:hyperhtml build:polymer3 build:riot build:skatejs:lithtml build:skatejs:preact build:slim build:vue"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vogloblinsky/web-components-benchmark.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vogloblinsky/web-components-benchmark/issues"
    },
    "homepage": "https://github.com/vogloblinsky/web-components-benchmark#readme",
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}